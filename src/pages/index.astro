---
import BaseHead from '../components/BaseHead.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { SITE_TITLE, SITE_DESCRIPTION } from '../consts';


---


<!doctype html>
<html lang="en">
	<head>
		<BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
	</head>
	<body>
		<Header />
		<main>
			<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2616.9948478283413!2d8.393480876222517!3d49.01068298997732!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x4797071e53c1c5b1%3A0x23bd7da43c47d949!2sChao*21%20Vietnam!5e0!3m2!1sde!2sde!4v1734918477908!5m2!1sde!2sde" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade">
            </iframe>
		</main>	
		<Footer />
	</body>
</html>
<style>
	main {
		width: 960px;
	}
	iframe {
		width: 100%;
		height: 450px;
	}
	
</style>